{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "program": "${workspaceFolder}/Api/App.ts",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Attach",
      "type": "node",
      "port": 9229,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Attach to Docker",
      "type": "node",
      "request": "attach",
      "restart": true,
      "port": 9229,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/wishlist-api",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
