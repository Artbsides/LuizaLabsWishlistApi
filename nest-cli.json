{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "Api",
  "entryFile": "App",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      {
        "name": "@nestjs/swagger/plugin",
        "options": {
          "dtoFileNameSuffix": "Dto.ts",
          "controllerFileNameSuffix": "Controller.ts",
          "classValidatorShim": true
        }
      }
    ]
  }
}
